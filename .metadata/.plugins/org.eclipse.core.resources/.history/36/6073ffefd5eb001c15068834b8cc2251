
public class Bucles {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int edad = 1;
		// mientras que edad sea menor que 18
		while (edad < 18) {
		System.out.printf("%d", edad).println();
		edad++;
		}
		System.out.printf("Programa finalizado --> edad: %d", edad).println();
	}

}
