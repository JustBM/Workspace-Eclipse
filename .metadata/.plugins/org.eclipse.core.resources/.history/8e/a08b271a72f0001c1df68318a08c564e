package com.examen.ja;

public class Colibri extends Animales {

	//Atributos			
			private float velocidad;
			private boolean plumas;
	//Constructor
			public Colibri () {
				//super(0.0f, 4, 0);
				this.velocidad=0;
				this.plumas=true;
			}
			public Colibri(float velocidad, boolean plumas) {
				//super (velocidad , asientos , orientacionRuedas);
				this.velocidad=velocidad;
				this.plumas=plumas;
			}
				
	//Metodos
		public void volar () {}
		public void posarse () {}

}
