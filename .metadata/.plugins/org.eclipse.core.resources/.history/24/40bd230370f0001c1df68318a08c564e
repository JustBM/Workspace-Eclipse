package com.examen.ja;

public class Leopardo {
	
	//atributos			
	private float velocidad;
	private int numeroDePelos;

		public Leopardo () {
			//super(0.0f, 4, 0);
			this.velocidad=0;
			this.numeroDePelos=4;
		}
		public Leopardo(float velocidad, int numeroDePelos) {
			//super (velocidad , asientos , orientacionRuedas);
			this.velocidad=velocidad;
			this.numeroDePelos=numeroDePelos;
		}
		

//metodos
public void acelerar () {}
public void morder () {}

}
