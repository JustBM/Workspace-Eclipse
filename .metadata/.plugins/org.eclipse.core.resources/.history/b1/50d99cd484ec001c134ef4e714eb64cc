package com.string.info;

public class String1 {

	public static void main(String[] args) {
		
	System.out.println("f" + "u" + "l" + "l" + "s" + "t" + "a" + "c" + "k"  );
	
	char f = 'F';
	char u = 'u';
	char l = 'l';
	char s = 's';
	char t = 't';
	char a = 'a';
	char c = 'c';
	char k = 'k';
	
	System.out.println(f + u + l + l + s + t + a + c + k);
	System.out.println(f);
	System.out.println(u);
	System.out.println(l );
	System.out.println(l);
	System.out.println(s);
	System.out.println(t);
	System.out.println(a);
	System.out.println(c);
	System.out.println(k);
	
	String deletreo1 = "F";
	String deletreo2 = "u";
	String deletreo3 = "l";
	String deletreo4 = "l";
	String deletreo5 = "s";
	String deletreo6 = "t";
	String deletreo7 = "a";
	String deletreo8 = "c";
	String deletreo9 = "k";
	
	System.out.println(deletreo1 + " " + deletreo2 + " " + deletreo3 + " " + deletreo4 + " " 
						+ deletreo5 + " " + deletreo6 + " " + deletreo7 + " " + deletreo8 + " " 
						+ deletreo9);
	}
}
